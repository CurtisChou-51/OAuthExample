<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>OAuthExample.Web</title>
    <link rel="stylesheet" href="~/OAuthExample.Web.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="header-function">
        </div>
        <div class="header-function login">
            @await Component.InvokeAsync("UserInfo")
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
</body>
</html>
